
<%= render('layouts/errors', object: @tag)%>

<%= form_for @tag do |f| %>
  <div class="control-group">
      <%= f.label :tag %>
      <div class="controls">
        <%= f.text_field :tag %>
      </div>
    <div class="control-group">
      <div class="controls">
        <%= f.submit 'submit', class: 'btn btn-primary'%>
      </div>
    </div>
<% end %>